<script>
    // Javascript snippet to remove tabIndex to avoid keyboard focus to the Video element.
    window.onload = function () {
        var heroHomepageCobalt = document.querySelector('.hero-homepage-cobalt-template__image');
        if (heroHomepageCobalt) {
            var videoButtonsWithTabIndex = heroHomepageCobalt.querySelectorAll('.w-css-reset button[tabIndex]');
            videoButtonsWithTabIndex.forEach((buttonwithTabIndex) => {
                buttonwithTabIndex.setAttribute('tabindex', '-1');
            });
        }
    }
</script>
