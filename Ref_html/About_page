        <div id="hero-cta-form-8f4555e3-82e0-4977-9fe2-ff031b7d4aec" class="panel-toggle hero_cta_container full-width">
                <div class="soft u-shadow bg-white">

<script>

    var requestType = requestType || [];

    $$epiforms(document).ready(function () {
        var formGuid = '5d0e2cd9-512e-4c4a-9142-eab508079608';
        var hasSubmitted = localStorage.getItem("formSubmitted-" + formGuid);

        if (hasSubmitted == "true") {
            $("#" + formGuid + " .Form__Status__Message").css("display", "none");
            $("#" + formGuid + " .form-steps-message").css("display", "flex");
            $("#" + formGuid + ".has-steps").css("padding-top", "0");
            $("#" + formGuid + " .Form__MainBody").css("display", "none");
        }
    });

    if (document.querySelector(".form-image-choice__option > input:checked") != null) {
        requestType[68804] = document.querySelector(".form-image-choice__option > input:checked").value;
    } else {
        requestType[68804] = 'Get in touch';
    }

    $$epiforms(document).ready(function () {
        var formGuid = '5d0e2cd9-512e-4c4a-9142-eab508079608';

        $$epiforms("#" + formGuid).on("click",
            function () {
                var currentStep = $(".Form__NavigationBar__ProgressBar__CurrentStep").text();
                var clickedOption = event.target.closest(".form-image-choice__option");
                if (clickedOption) {
                    var input = clickedOption.querySelector("input:checked");
                    if (input) {
                        requestType[68804] = input.value;
                        var hiddenField = document.getElementById("hiddenFieldRequestType");
                        hiddenField.value = input.value;
                    }
                }

                if (requestType[68804] == null && currentStep > 1) {
                    requestType[68804] = hiddenField = document.getElementById("hiddenFieldRequestType").value;
                } else if (requestType[68804] == null && currentStep == '') {
                    requestType[68804] = 'Get in touch';
                }
            });
    });

    $$epiforms(document).ready(function () {
        const pushFormStep = function () {
            
            let currentStep = $(".Form__NavigationBar__ProgressBar__CurrentStep").text();
    
            dataLayer.push({
                "event": "form_step",
                "form_request_type": requestType[68804],
                "form_step_number": currentStep,
                "form_step_title": "Get in touch",
                "form_step_description": "Complete the form to learn more about our solutions and discuss your business needs with a member of our team."
            });
            
        }

        var formGuid = '5d0e2cd9-512e-4c4a-9142-eab508079608';
        $$epiforms("#" + formGuid).on("formsNavigationNextStep", pushFormStep);
        $$epiforms("#" + formGuid).on("formsNavigationPrevStep", pushFormStep);
    });

    $$epiforms(document).ready(function () {
        var formGuid = '5d0e2cd9-512e-4c4a-9142-eab508079608';
        $$epiforms("#" + formGuid).on("formsSubmitted",
            function () {
                var lastStep = $("#" + formGuid + " .Form__NavigationBar__ProgressBar__StepsCount").text();
                var currentStep = $("#" + formGuid + " .Form__NavigationBar__ProgressBar__CurrentStep").text();
                var clickedOption = event.target.closest(".form-image-choice__option");
                if (clickedOption) {
                    var input = clickedOption.querySelector("input:checked");
                    if (input) {
                        requestType[68804] = input.value;
                        var hiddenField = document.getElementById("hiddenFieldRequestType");
                        hiddenField.value = input.value;
                    }
                }

                if (requestType[68804] == null && currentStep > 1) {
                    requestType[68804] = hiddenField = document.getElementById("hiddenFieldRequestType").value;
                } else if (requestType[68804] == null && currentStep == '') {
                    requestType[68804] = 'Get in touch';
                }
                 

                if (currentStep == lastStep || currentStep == '') {
                    var targetForm = document.getElementById(formGuid);
                    var nearestForm = targetForm.closest('#form');
                    var dataCtaLocationElement = null;
                    var dataCtaLocation = "";
                    var ctaLocationFound = false;
                    if (nearestForm != undefined && nearestForm != null) {
                        dataCtaLocationElement = nearestForm.querySelector('[data-cta-location]');

                        if (dataCtaLocationElement != undefined && dataCtaLocationElement != null) {
                            ctaLocationFound = true;
                            dataCtaLocation = dataCtaLocationElement.getAttribute("data-cta-location");
                        }
                        else {
                            var nearestNextForm = nearestForm.parentNode.closest('#form');
                            if (nearestNextForm != undefined && nearestNextForm != null) {
                                dataCtaLocationElement = nearestNextForm.querySelector('[data-cta-location]');
                                if (dataCtaLocationElement != undefined && dataCtaLocationElement != null) {
                                    ctaLocationFound = true;
                                    dataCtaLocation = dataCtaLocationElement.getAttribute("data-cta-location");
                                }
                            }
                        }
                    }
                    if (ctaLocationFound == false) {
                        var targetElem = targetForm.parentElement;
                        while (targetElem != undefined && targetElem != null) {
                            dataCtaLocationElement = targetElem.querySelector('[data-cta-location]');
                            if (dataCtaLocationElement != undefined && dataCtaLocationElement != null) {
                                targetElem = null;
                                ctaLocationFound = true;
                                dataCtaLocation = dataCtaLocationElement.getAttribute("data-cta-location");
                            }
                            else {
                                targetElem = targetElem.parentElement;
                            }
                        }
                    }
                    
                        dataLayer.push({
                            'fromUrl': window.location.href,
                            'formType': 'Pardot',
                            'event': 'formSubmission',
                            "form_location": dataCtaLocation
                        });
                    
                    dataLayer.push({
                        "event": "form_conversion",
                        "form_request_type": requestType[68804],
                        "form_step_number": currentStep,
                        "form_step_title": "Get in touch",
                        "form_step_description": "Complete the form to learn more about our solutions and discuss your business needs with a member of our team.",
                        "form_location": dataCtaLocation
                    });
                }

                 

            });
    });
</script>


<script>
    $$epiforms(document).ready(function () {
        var formGuid = '5d0e2cd9-512e-4c4a-9142-eab508079608';
        var numSteps = 1;
       
         $$epiforms("#" + formGuid).on("formsSubmitted",
             function () {
                 $("#" + formGuid + " .Form__MainBody").css("display", "block");

                 var currentStep = $(".Form__NavigationBar__ProgressBar__CurrentStep").text();
                 var lastStep = $(".Form__NavigationBar__ProgressBar__StepsCount").text();
                 var allowMultipleSubmission = 'True';

                 if ((currentStep == lastStep || numSteps == 0) && allowMultipleSubmission === 'False') {
                     localStorage.setItem("formSubmitted-" + formGuid, true);
                     $("#" + formGuid + " .Form__Status__Message").css("display", "none");
                     $("#" + formGuid + " .form-steps-message").css("display", "flex");
                     $("#" + formGuid + ".has-steps").css("padding-top", "0");
                     $("#" + formGuid + " .Form__MainBody").css("display", "none");
                 }
                 else if ((currentStep == lastStep || numSteps == 0) && allowMultipleSubmission === 'True') {
                     $("#" + formGuid + " .Form__Status__Message").css("display", "none");
                     $("#" + formGuid + " .form-steps-message").css("display", "flex");
                     $("#" + formGuid + ".has-steps").css("padding-top", "0");
                     $("#" + formGuid + " .Form__MainBody").css("display", "none");
                 }
             });
    });
</script>


