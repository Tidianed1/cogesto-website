<script>

    window.dataLayer = window.dataLayer || [];

    var registerClicks = function () {
        

        var querySelector = '[data-tracking-click],[ga4-data-tracking-click]';
        var dataSelector = document.querySelectorAll(querySelector);
        if (dataSelector !== undefined && dataSelector !== null) {
            for (var i = 0; i < dataSelector.length; ++i) {
                dataSelector[i].onclick = function() {
                    var data = this.getAttribute('data-tracking-click');
                    if (data !== undefined && data !== null) {
                            data = window.atob(data);

                        data = JSON.parse(data);

                        
                        

                        dataLayer.push(data);
                    }

                    var ga4data = this.getAttribute('ga4-data-tracking-click');
                    if (ga4data !== undefined && ga4data !== null) {
                            ga4data = window.atob(ga4data);

                        ga4data = JSON.parse(ga4data);

                        
                        

                        dataLayer.push(ga4data);
                    }
                }
            }
        }
    }

    registerClicks();

</script>






